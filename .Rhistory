View(new.mtcars)
ggplot(data =new.mtcars, aes(x=new.mtcars$PC1, y = new.mtcars$PC2,  color = cyl)) + geom_point() +geom_text(aes(label=rownames(mtcars),hjust=0, vjust=0))
ggplot(data =new.mtcars, aes(x=new.mtcars$PC1, y = new.mtcars$PC2,  color = mtcars$cyl) + geom_point() +geom_text(aes(label=rownames(mtcars),hjust=0, vjust=0))
ggplot(data =new.mtcars, aes(x=new.mtcars$PC1, y = new.mtcars$PC2,  color = mtcars$cyl) + geom_point() +geom_text(aes(label=rownames(mtcars),hjust=0, vjust=0))
ggplot(data =new.mtcars, aes(x=new.mtcars$PC1, y = new.mtcars$PC2,  color = mtcars$cyl)) + geom_point() +geom_text(aes(label=rownames(mtcars),hjust=0, vjust=0))

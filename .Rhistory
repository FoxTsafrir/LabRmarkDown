library(summaryPack)
mtcars.pca <- prcomp(mtcars[,c(1:7,10,11)], center = TRUE, scale. = TRUE)
summary(mtcars.pca)
library(ggplot2)
ggplot(data = mtcars.pca, aes(x=PC1, y = PC2))
ggplot(data = mtcars.pca, aes(x=PC1, y = PC2)) + geom_point()
as.data.frame(mtcars.pca$x)
new.mtcars=as.data.frame(mtcars.pca$x)
ggplot(data =new.mtcars, aes(x=new.mtcars$PC1, y = new.mtcars$PC1)) + geom_point()
ggplot(data =new.mtcars, aes(x=new.mtcars$PC1, y = new.mtcars$PC2)) + geom_point()
cor.test(new.mtcars$PC1,mtcars$mpg)
cor.test(new.mtcars$PC1,mtcars$cyl)
+geom_text(aes(label=rownames(mtcars),hjust=0, vjust=0))
